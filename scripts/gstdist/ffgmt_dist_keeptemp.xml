<gstbuildpars>
    <srcdirs>
        <srcdir>build/bin</srcdir>
        <srcdir>build/lib</srcdir>
        <srcdir>src/vgtmpeg</srcdir>
        <srcdir>src/vgtmpeg/doc</srcdir>
    </srcdirs>
    <dstdirs>
        <dstdir name="bindir">bin</dstdir>
        <dstdir name="libdir">lib</dstdir>
        <dstdir name="plugindir">lib</dstdir>
        <dstdir name="doc">doc</dstdir>
    </dstdirs>
    <group name="dummy_group">
    </group>
    <group name="gst_devel">
        <item type="bin" dest="bindir">vgtmpeg</item>
        <item type="plain" dest="doc">vgtmpeg.html</item>
        <item type="plain" dest="doc">README.md</item>
    </group>
    <deploy> 
        <dgroup>gst_devel</dgroup>
    </deploy>
    <options>
        <tardist nbundle="on" tar="on" sevenzip="off" gzip="off" bzip2="off" >gmt-gstdist</tardist> 
        <cleanfirst/>
        <keeptemp/> 
    </options>
</gstbuildpars>
